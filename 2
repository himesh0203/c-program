#include <iostream>
using namespace std;

int main()
{
    int N, i, j;
    bool isPrime;

    cout << "Enter the value of N to find prime numbers up to: ";
    cin >> N;

    for (i = 2; i <= N; i++)
    {
        isPrime = true;

        for (j = 2; j <= i / 2; j++)
        {
            if (i % j == 0)
            {
                isPrime = false;
                break;      // â† Just break the loop
            }
        }

        if (isPrime)
            cout << i << " ";
    }

    return 0;
}

